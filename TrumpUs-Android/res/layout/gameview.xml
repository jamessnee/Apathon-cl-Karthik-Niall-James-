        <Button android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceLarge" android:id="@+id/metric1" android:text="Metric1" android:layout_width="fill_parent"></Button>
        <Button android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceLarge" android:id="@+id/metric2" android:text="Metric2" android:layout_width="fill_parent"></Button>
    	<Button android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceLarge" android:id="@+id/metric3" android:text="Metric3" android:layout_width="fill_parent"></Button>
    	<Button android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceLarge" android:id="@+id/metric4" android:text="Metric4" android:layout_width="fill_parent"></Button>
    	<Button android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceLarge" android:id="@+id/metric5" android:text="Metric5" android:layout_width="fill_parent"></Button>
    </LinearLayout>
    
